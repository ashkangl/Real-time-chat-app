{
    "version": 2,
    "builds": [
    {
    "src": "server/index.js",
    "use": "@vercel/node"
    },
    {
    "src": "public/**/*",
    "use": "@vercel/static"
    }
    ],
    "routes": [
    {
    "src": "/public/(.*)",
    "dest": "/public/$1"
    },
    {
    "src": "/(.*)",
    "dest": "/server/index.js"
    }
    ]
    }
    