{
    "version": 2,
    "builds": [
    {
    "src": "server/index.js",
    "use": "@vercel/node"
    },
    {
    "src": "package.json",
    "use": "@vercel/static-build",
    "config": {
    "distDir": "build"
    }
    }
    ],
    "rewrites": [
    {
    "source": "/api/(.*)",
    "destination": "server"
    },
    {
    "source": "/(.*)",
    "destination": "/$1"
    }
    ]
    }